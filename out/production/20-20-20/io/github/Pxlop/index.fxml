<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<VBox alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" spacing="10.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="io.github.Pxlop.MainController">
    <Text style="-fx-text-alignment: center; -fx-font-size: 36; -fx-font-weight: bold;" text="20-20-20" textAlignment="CENTER" />
    <Label fx:id="timerText" style="-fx-text-alignment: center; -fx-font-family: sans-serif; -fx-font-size: 24; -fx-padding: 10;" textAlignment="CENTER" />
    <HBox alignment="CENTER" spacing="10.0">
        <Button fx:id="startButton" alignment="CENTER" style="-fx-background-radius: 0;" text="Start" textAlignment="CENTER" />
        <Button fx:id="stopButton" alignment="CENTER" mnemonicParsing="false" style="-fx-background-radius: 0;" text="Stop" textAlignment="CENTER" />
    </HBox>
    <Button fx:id="settingsButton" alignment="CENTER" mnemonicParsing="false" style="-fx-background-radius: 0;" text="Settings" textAlignment="CENTER" />
</VBox>
